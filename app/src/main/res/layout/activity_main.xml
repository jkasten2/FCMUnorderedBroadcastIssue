<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/send_3rd_party_push"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onSendOtherSDKPushClicked"
        android:text="Send Other SDK Push"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/send_onesignal_push" />

    <Button
        android:id="@+id/send_onesignal_push"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onSendOneSignalPushClicked"
        android:text="Send Onesignal Push"
        app:layout_constraintBottom_toTopOf="@+id/send_3rd_party_push"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>